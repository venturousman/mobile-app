  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to packager",
      "request": "attach",
      "type": "reactnative",
      "cwd": "${workspaceFolder}",
      "port": 19000
      // "program": "${workspaceRoot}/.vscode/launchReactNative.js",
      // "sourceMaps": true,
      // "outDir": "${workspaceRoot}/.vscode/.react"
    },
    {
      "name": "Debug Android",
      "cwd": "${workspaceFolder}",
      "type": "reactnative",
      "request": "launch",
      "platform": "android",
      "program": "${workspaceRoot}/.vscode/launchReactNative.js",
      "sourceMaps": true,
      "outDir": "${workspaceRoot}/.vscode/.react"
    }
  ]
}
